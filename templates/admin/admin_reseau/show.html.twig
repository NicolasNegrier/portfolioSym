{% extends 'base.html.twig' %}

{% block title %}Administration Reseau
{% endblock %}

{% block body %}

	<div class="section__title">
		<h2>Détails du réseau
			{{ reseau.name}}
		</h2>
		<a href="{{ path('admin_reseau_edit', { 'id': reseau.id }) }}" type="button">Modifier</a>
	</div>

	<div class="section__item">Nom :
		{{ reseau.name}}</div>
	<div class="section__item">Logo :
		{{ reseau.logo}}</div>
	<div class="section__item">URL du profil :
		{{ reseau.url}}</div>

{% endblock %}
